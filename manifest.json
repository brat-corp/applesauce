{
    "manifest_version": 2,
    "name": "Getting started example",
    "description": "This extension shows a Google Image search result for the current page",
    "version": "1.0",
    "background": {
        "persistent": true,
        "scripts": [
            "src/js/http-request/http-request.js",
            "src/js/dictionary/dictionary.js",
            "src/js/main.js"
        ]
    },
    "permissions": [
        "http://*/",
        "https://*/"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}